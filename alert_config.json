    },
    "telegram": {
        "enabled": true,
        "bot_token": "your-bot-token",
        "chat_id": "your-chat-id"
    },
    "thresholds": {
        "cpu_usage": 80,
        "memory_usage": 80,
        "inference_time": 1.0,
        "error_rate": 1.0
    },
    "alert_cooldown": 300,
    "alert_levels": {
        "performance": "WARNING",
        "resources": "WARNING",
        "errors": "ERROR",
        "system": "INFO"
    }
