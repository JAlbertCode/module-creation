{
  "name": "production_resnet",
  "description": "Production configuration for ResNet models with optimized settings",
  "model_type": "image-classification",
  "config": {
    "resources": {
      "cpu": 4,
      "memory": "8Gi",
      "gpu": 1
    },
    "model": {
      "batch_size": 16,
      "half_precision": true
    },
    "input": {
      "preprocessing": {
        "resize": [384, 384],
        "normalize": true,
        "center_crop": true
      }
    },
    "output": {
      "format": "json",
      "path": "/outputs/result.json"
    },
    "optimization": {
      "enable_tensorrt": true,
      "dynamic_batching": true,
      "cache_warming": true
    }
  },
  "created_at": "2025-01-20T10:00:00",
  "last_used": "2025-01-20T10:00:00"
}