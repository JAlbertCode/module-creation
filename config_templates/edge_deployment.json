{
  "name": "edge_deployment",
  "description": "Optimized for edge devices with limited resources",
  "model_type": "image-classification",
  "config": {
    "resources": {
      "cpu": 1,
      "memory": "2Gi",
      "gpu": null
    },
    "model": {
      "batch_size": 1,
      "half_precision": true,
      "quantization": {
        "enabled": true,
        "type": "int8"
      }
    },
    "input": {
      "preprocessing": {
        "resize": [224, 224],
        "normalize": true
      }
    },
    "optimization": {
      "enable_onnx": true,
      "enable_cache": true,
      "thread_limit": 2
    }
  },
